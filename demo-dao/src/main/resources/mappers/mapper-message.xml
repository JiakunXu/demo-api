<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.bytedance.dao.mapper.MessageMapper">

    <insert id="insertMessage" parameterType="message" useGeneratedKeys="true" keyProperty="id">
	<![CDATA[
		INSERT
		INTO `tb_message`
		  (
			`to_user_name`,
			`from_user_name`,
			`create_time`,
			`msg_type`,
			`content`,
			`pic_url`,
			`is_deleted`,
			`creator`,
			`create_time`
		  )
		  VALUES
		  (
			#{toUserName},
			#{fromUserName},
			#{createTime},
			#{msgType},
			#{content},
			#{picUrl},
			0,
			#{creator},
			NOW()
		  )
	]]>
    </insert>

</mapper>